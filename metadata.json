      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppetfinland/nginx",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetfinland/os",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppetfinland/pf_postgresql",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppetfinland/sslcert",
      "version_requirement": ">= 0.1.0 < 1.0.0"
    },
    {
      "name": "camptocamp/systemd",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/git",
